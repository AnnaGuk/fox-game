{
  "name": "fox-game",
  "version": "1.0.0",
  "description": "Tamagochi-like game",
  "scripts": {
    "dev": "parcel src/index.html",
    "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,css}\"",
    "format:check": "prettier --ignore-path ./.gitignore --check \"./**/*.{html,json,js,ts,css}\"",
    "lint": "eslint --ignore-path ./.gitignore --fix \"./**/*.{js,ts}\"",
    "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaGuk/fox-game.git"
  },
  "keywords": [
    "tamagochi",
    "game"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnnaGuk/fox-game/issues"
  },
  "homepage": "https://github.com/AnnaGuk/fox-game#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.1",
    "jest-esm-transformer": "^1.0.0",
    "parcel": "^2.1.1",
    "prettier": "^2.1.2"
  },
  "overrides": {
    "node-forge": "^1.0.0",
    "postcss": "^8.4.5"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "jest": {
    "transform": {
      "\\.m?jsx?$": "jest-esm-transformer"
    }
  }
}
